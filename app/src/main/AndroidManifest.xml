<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moneytrakingsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoneyTrakingSystem">
        <activity android:name="com.example.MTS.DailyAnalyticsActivity"></activity>
        <activity android:name="com.example.MTS.ChooseAnalyticActivity" />
        <activity android:name="com.example.MTS.WeekSpendingActivity" />
        <activity android:name="com.example.MTS.TodaySpendingActivity" />
        <activity android:name="com.example.MTS.BudgetActivity" />
        <activity android:name="com.example.MTS.MainActivity" />
        <activity android:name="com.example.MTS.RegistrationActivity" />
        <activity android:name="com.example.MTS.LoginActivity" />
        <activity android:name="com.example.MTS.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>